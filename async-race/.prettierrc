{
  "arrowParent": "avoid",
  "useTabs": false,
  "singleQuote": true,
  "tabWidth": 2,
  "semi": true,
  "trailingComma": "none",
  "importOrder": [
    "<THIRD_PARTY_MODULES>",
    "^@components/(.*)$",
    "^[./]",
    "^../(.*)",
    "^./(.*)"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}